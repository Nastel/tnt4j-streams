<?xml version="1.0" encoding="utf-8"?>
<tnt-data-source
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="..\..\..\config\tnt-data-source.xsd">

    <parser name="ExampleTokenParser" class="com.jkool.tnt4j.streams.parsers.ActivityTokenParser">
        <property name="FieldDelim" value=","/>
        <field name="ServerName" locator="1"/>
        <field name="Tag" locator="8"/>
        <field name="EventName" locator="9"/>
        <field name="EventType" locator="10"/>
        <field name="ElapsedTime" locator="11" units="Microseconds"/>
        <field name="StartTime" locator="13" format="yyyy-MM-dd HH:mm:ss.SSSSSS"/>
        <field name="CompCode" locator="14"/>
        <field name="ReasonCode" locator="15"/>
        <field name="ResourceName" locator="16"/>
        <field name="UserName" locator="18"/>
    </parser>

    <stream name="ExampleFileStream" class="com.jkool.tnt4j.streams.inputs.FileLineStream">
        <property name="FileName" value="orders.log"/>
        <parser-ref name="ExampleTokenParser"/>
    </stream>
</tnt-data-source>
